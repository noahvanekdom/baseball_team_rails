<%= form_with url: "/teams/#{@team.id}", method: :patch, local: true do |form| %>
<%= form.label :name %>
<%= form.text_field :team_name %>
<%= form.label :city %>
<%= form.text_field :city %>
<%= form.label :win_percent %>
<%= form.text_field :win_percent %>
<%= form.label :division_leader? %>
<%= form.text_field :division_leader %>
<%= form.submit "Update Team" %>
<% end %>